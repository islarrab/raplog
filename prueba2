def fibonacci(int n) {
    if n > 2
    {
    	f1 = 0
    	f2 = 0
    	f1 = fibonacci(n - 1)
    	f2 = fibonacci(n - 2)
    	return f1 + f2
    } else {
    	return 1
    }
}

def factorial(int n) {
	if n <= 1 {
		return n
	} else {
		f1 = factorial(n - 1)
		return f1 * n
	}
}

a = 1

