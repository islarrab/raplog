fibonacci(in n, out f) {
    if n >= 2
    {
    	fibonacci(n - 1, f1)
    	fibonacci(n - 2, f2)
    	f = f1 + f2
    } else {
    	f = n
    }
}

factorial(in n, out f)
	if n <= 1
	{
		f = n
	} else {
		factorial(n - 1, f1)
		f = f * f1
	}
}

start {
	_asd123ASD = 1
	ABC_DEF = 2
