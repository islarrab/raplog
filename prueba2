def fibonacci_aux(n, p0, p1) {
	print ", "
	print p1
	if (n > 1) {
        fibonacci_aux(n-1, p1, p0+p1)
    }
}

def fibonacci(n) {
    print "0"
    if n > 0 {
    	fibonacci_aux(n-1, 0, 1)
	}
	print "\n"
}

def factorial(n) {
	if n <= 1 {
		return n
	} else {
		f1 = factorial(n - 1)
		return f1 * n
	}
}

def main (){
	print "secuencia fibonacci hasta el 10Â° numero\n"
	fibonacci(10)
	res = factorial(10)
	print "factorial de 10 es "
	print res
	print "\n"
}
main()
