	put "secuencia fibonacci hasta el 10Â° numero"
	i = 1
	loop i <= 10 {
		fibonacci(i, res)
		put res+", "
		i = i + 1
	}
	
	
	factorial(10, res)
	put "factorial de 10 es "+res
}
