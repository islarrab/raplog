fibonacci(in n, out f) {
    if n > 2
    {
    	f1 = 0
    	f2 = 0
    	fibonacci(n - 1, f1)
    	fibonacci(n - 2, f2)
    	f = f1 + f2
    } else {
    	f = 1
    }
}

factorial(in n, out f) {
	if n <= 1
	{
		f = n
	} else {
		f1 = 0
		factorial(n - 1, f1)
		f = f * f1
		variable1 = 2
	}
}

